{
  "name": "@mathrix-education/setup-sentry-cli",
  "version": "1.0.0",
  "description": "Install the Sentry CLI in your GitHub Actions workflow.",
  "keywords": [
    "github-actions",
    "actions",
    "sentry",
    "sentry-cli",
    "sentry-release"
  ],
  "homepage": "https://github.com/mathrix-education/setup-sentry-cli",
  "bugs": {
    "url": "https://github.com/mathrix-education/setup-sentry-cli/issues",
    "email": "mathieu@mathrix.fr"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "author": {
    "name": "Mathieu Bour",
    "email": "mathieu@mathrix.fr"
  },
  "browser": false,
  "repository": "github:mathrix-education/setup-sentry-cli",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "build": "npm run clean && ncc build --source-map --no-source-map-register ./src/index.ts"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "private": true
}
