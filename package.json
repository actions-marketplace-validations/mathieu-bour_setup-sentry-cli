{
  "name": "@mathieu-bour/setup-sentry-cli",
  "version": "1.2.0",
  "description": "Install the Sentry CLI in your GitHub Actions workflow.",
  "keywords": [
    "github-actions",
    "actions",
    "sentry",
    "sentry-cli",
    "sentry-release"
  ],
  "homepage": "https://github.com/mathieu-bour/setup-sentry-cli",
  "bugs": {
    "url": "https://github.com/mathieu-bour/setup-sentry-cli/issues",
    "email": "mathieu@mathrix.fr"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "author": {
    "name": "Mathieu Bour",
    "email": "mathieu@mathrix.fr"
  },
  "browser": false,
  "repository": "github:mathieu-bour/setup-sentry-cli",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "prebuild": "npm run clean",
    "build": "node .esbuildrc.js"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^5.0.0",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "esbuild": "^0.8.34",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-commit": "^4.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "private": true
}
